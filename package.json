{
  "name": "vtx-api",
  "version": "1.0.0",
  "description": "WebAPI for Vtx Frequency Calculation and Vtx/Manufacturer Information",
  "main": "/src/server/server.js",
  "scripts": {
    "start": "node src/server/server.js",
    "start-watch": "nodemon --exec \"npm start\"",
    "test:mocha": "export NODE_ENV=test && mocha src/server/**/*.test.js",
    "test:mocha:watch": "nodemon --exec \"npm test:mocha\"",
    "test:ava": "export NODE_ENV=test && ava",
    "test:ava:watch": "nodemon --exec \"npm test:ava\""
  },
  "engines": {
    "node": "6.9.0"
  },
  "author": "JÃ¶rg S.",
  "license": "GPLv3",
  "dependencies": {
    "bcryptjs": "^2.4.0",
    "body-parser": "^1.15.2",
    "crypto-js": "^3.1.9-1",
    "express": "^4.14.0",
    "js-combinatorics": "^0.5.2",
    "jsonwebtoken": "^7.2.1",
    "lodash": "^4.17.4",
    "mongodb": "^2.2.19",
    "mongoose": "^4.7.6",
    "mongoose-type-url": "^1.0.2",
    "validator": "^6.2.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.0",
    "@types/chai-as-promised": "0.0.29",
    "@types/chai-subset": "^1.3.0",
    "@types/crypto-js": "^3.1.32",
    "@types/expect": "^1.20.0",
    "@types/express": "^4.0.34",
    "@types/js-combinatorics": "^0.5.29",
    "@types/jsonwebtoken": "^7.2.0",
    "@types/lodash": "^4.14.48",
    "@types/mocha": "^2.2.38",
    "@types/mongodb": "^2.1.37",
    "@types/mongoose": "^4.7.2",
    "@types/rewire": "^2.5.27",
    "@types/supertest": "^2.0.0",
    "@types/validator": "^5.7.35",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chai-subset": "^1.4.0",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "supertest": "^2.0.1"
  },
  "ava": {
    "files": [
      "test-*.js",
      "test/**/*.js",
      "**/*.test.js"
    ],
    "source": [
      "**/*.{js,jsx}",
      "!dist/**/*"
    ],
    "concurrency": 5,
    "failFast": false,
    "verbose": true,
    "powerAssert": false
  }
}
